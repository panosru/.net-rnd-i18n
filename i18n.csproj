<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>



  <ItemGroup>
    <Folder Include="Resources\" />
    <Folder Include="Resources\Pages\" />
    <Folder Include="Localization\" />
    <Folder Include="TagHelpers\" />
    <Folder Include="Middlewares\" />
    <Folder Include="Resources\Localization\" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Resources\Pages\Privacy.en-GB.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Privacy.en-GB.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Pages\Privacy.en-US.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Privacy.en-US.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Condition=" '$(EnableDefaultEmbeddedResourceItems)' == 'true' " Update="Resources\Localization\SharedResource.en-GB.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>SharedResource.en-GB.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Condition=" '$(EnableDefaultEmbeddedResourceItems)' == 'true' " Update="Resources\Localization\SharedResource.en-US.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>SharedResource.en-US.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Pages\IndexModel.en-GB.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Index.en-GB.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Pages\IndexModel.en-US.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Index.en-US.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Pages\IndexModel.en-GB.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>IndexModel.en-GB.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Pages\IndexModel.en-US.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>IndexModel.en-US.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Resources\Localization\SharedResource.en-US.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Resources\Pages\Privacy.en-US.Designer.cs">
      <DependentUpon>Privacy.en-US.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Pages\Privacy.en-GB.Designer.cs">
      <DependentUpon>Privacy.en-GB.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Pages\Privacy.en-US.Designer.cs">
      <DependentUpon>Privacy.en-US.resx</DependentUpon>
    </Compile>
    <Compile Condition=" '$(EnableDefaultCompileItems)' == 'true' " Update="Resources\Localization\SharedResource.en-GB.Designer.cs">
      <DependentUpon>SharedResource.en-GB.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Pages\Index.en-US.Designer.cs">
      <DependentUpon>Index.en-US.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Pages\IndexModel.en-GB.Designer.cs">
      <DependentUpon>IndexModel.en-GB.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Pages\IndexModel.en-US.Designer.cs">
      <DependentUpon>IndexModel.en-US.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\Pages\Index.en-GB.Designer.cs">
      <DependentUpon>Index.en-GB.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Remove="Resources\Pages\Privacy.en-GB" />
    <None Remove="Resources\Pages\Privacy.en-US" />
  </ItemGroup>
</Project>